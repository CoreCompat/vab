{

  "version": "6.0.1304-*",

  "authors": [ "Microsoft" ],
  "title": "Enterprise Library - Validation Application Block for .NET Core",
  "description": "The Enterprise Library Validation Application Block provides useful features that allow developers to implement structured and easy-to-maintain validation scenarios in their applications.",

  "packOptions": {
    "licenseUrl": "https://github.com/CoreCompat/vab/blob/master/LICENSE.txt",
    "owners": [ "quamotion" ],
    "repository": {
      "type": "git",
      "url": "https://github.com/CoreCompat/vab/"
    }
  },

  "dependencies": {
  },

  "buildOptions": {
    "keyFile": "../CoreCompat.EnterpriseLibrary.Common.snk",
    "strongName": true,
    "compile": {
      "include": [
        "../Source/Src/Validation/**/*.cs",
        "../Source/Source/GlobalAssemblyInfo.cs"
      ],
      "exclude": [
        "../Source/Src/Validation/Configuration/**/*.cs",
        "../Source/Src/Validation/ConfigurationValidatedElement.cs",
        "../Source/Src/Validation/ConfigurationValidatedType.cs",
        "../Source/Src/Validation/ConfigurationValidatorBuilder.cs",
        "../Source/Src/Validation/ConfigurationValidatorFactory.cs"
      ]
    }
  },

  "frameworks": {
    "netstandard1.5": {
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-24027",
        "System.ComponentModel.Annotations": "4.1.0-rc2-24027",
        "CoreCompat.System.ComponentModel.DataAnnotations": "4.2.3-r2"
      },
      "buildOptions": {
        "define": [ "CORECLR", "SIGN" ]
      }
    }
  },

  "runtimes": {
    "win7-x64": { }
  }
}
